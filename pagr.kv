#: import FloatLayout kivy.uix.floatlayout.FloatLayout

<Mygtukas@Button>:
    font_size: '20sp'
    color: 1, 1, 1, .8
    size_hint: 0.2, 0.1

<MygtukasV@Button>:
    color: 0, 1, 0, 1
    size_hint: 0.04, .19
    background_color: 0, .7, 0, 1

<Tabbedpanel>:
    canvas:
        Color:
            rgba: 1, 1, 1, .7
        Rectangle:
            size: self.size
            pos: self.pos
    size_hint: 1, 1 #procentai: 1 - 100%, .5 - 50%
    pos_hint: {'center_x': .5, 'center_y': .5}
    do_default_tab: False
#--------------------------------------------------pirmas tabas--------------------------------------------
    TabbedPanelItem:
        text: 'Value'
        canvas:
            Color:
                rgba: 1, 1, 1, .6
            Rectangle:
                size: self.size
                pos: self.pos
        FloatLayout:
            RstDocument:
                text: "Calculates the resistance value according to color-code."
                size_hint: 1, .1

            Mygtukas:
                id: keturiosj
                text: "4-band-code"
                pos_hint: {"x": 0, 'y': .9}
                on_press: VarzMyg3.disabled=True
                on_press: VarzMyg6.disabled=True
                on_press: VarzMyg3.background_color=(0, .7, 0, 0)
                on_press: VarzMyg6.background_color=(0, .7, 0, 0)
                on_press: VarzMyg4.pos_hint={"x": 0.49, 'y': .455}
                on_press: VarzMyg5.pos_hint={"x": 0.67, 'y': .455}

            Mygtukas:
                id: penkiosj
                text: "5-band-code"
                pos_hint: {"x": .2, 'y': .9}
                on_press: VarzMyg3.disabled=False
                on_press: VarzMyg6.disabled=True
                on_press: VarzMyg3.background_color=(0, .7, 0, 1)
                on_press: VarzMyg6.background_color=(0, .7, 0, 0)
                on_press: VarzMyg4.pos_hint={"x": 0.56, 'y': .455}
                on_press: VarzMyg5.pos_hint={"x": .67, 'y': .455}

            Mygtukas:
                id: sesiosj
                text: "6-band-code"
                pos_hint: {"x": .4, 'y': .9}
                on_press: VarzMyg3.disabled=False
                on_press: VarzMyg6.disabled=False
                on_press: VarzMyg3.background_color=(0, .7, 0, 1)
                on_press: VarzMyg6.background_color=(0, .7, 0, 1)
                on_press: VarzMyg4.pos_hint={"x": 0.56, 'y': .455}
                on_press: VarzMyg5.pos_hint={"x": 0.63, 'y': .455}

            Image:
                allow_stretch: True
                source: "Resistor_symbol.png"
                pos_hint: {"x": 0.25, 'y': .25}
                size_hint: .6, .6

            Label:
                background_color: 1, 1, 1, .3
                text: root.rezultatas()
                base_font_size: '30sp'
                size_hint: .15, .1
                pos_hint: {"x": .45, "y": .25}
#-----------------------------------------------Varzo mygtukai------------
            MygtukasV:
                id: VarzMyg1                 #varzo mygtukas nr.1
                #text: "1"
                pos_hint: {"x": 0.35, 'y': .455}
                background_normal: ''
                #on_press: VarzMyg1.background_color=root.popupwindow1()
                on_press: root.popupwindow1()


            MygtukasV:
                id: VarzMyg2
                #text: "2"
                pos_hint: {"x": 0.42, 'y': .455}
                background_normal: ''
                on_press: root.popupwindow1()

            MygtukasV:
                id: VarzMyg3
                #text: "3"
                pos_hint: {"x": 0.49, 'y': .455}
                background_normal: ''
                on_press: root.popupwindow1()

            MygtukasV:
                id: VarzMyg4
                #text: "4"
                pos_hint: {"x": 0.56, 'y': .455}
                background_normal: ''
                on_press: root.popupwindow2()

            MygtukasV:
                id: VarzMyg5
                #text: "5"
                pos_hint: {"x": 0.63, 'y': .455}
                background_normal: ''
                on_press: root.popupwindow3()

            MygtukasV:
                id: VarzMyg6
                #text: "6"
                pos_hint: {"x": 0.7, 'y': .455}
                background_normal: ''
                on_press: root.popupwindow4()
#----------------------------------------------------antras-tabas--------------------------------------------------
    TabbedPanelItem:
        text: 'Colors'
        canvas:
            Color:
                rgba: 1, 1, 1, .6
            Rectangle:
                size: self.size
                pos: self.pos
        FloatLayout:
            RstDocument:
                text: "Calculates the color-code according to the value of the resistance."
                size_hint: 1, .1

            Mygtukas:
                id: keturiosjuost
                text: "4-band-code"
                pos_hint: {"x": 0, 'y': .9}

            Mygtukas:
                id: penkiosjuost
                text: "5-band-code"
                pos_hint: {"x": .2, 'y': .9}

            Image:
                allow_stretch: True
                source: "Resistor_symbol.png"
                pos_hint: {"x": 0.25, 'y': .25}
                size_hint: .6, .6

            Label:
                background_color: 1, 1, 1, .3
                text: 'tralialia'
                base_font_size: '30sp'
                size_hint: .15, .1
                pos_hint: {"x": .45, "y": .25}
#-----------------------------------------------trecias tabas----------------
    TabbedPanelItem:
        text: 'Voltage\nDivider'
        canvas:
            Color:
                rgba: 1, 1, 1, .6
            Rectangle:
                size: self.size
                pos: self.pos
        FloatLayout:
            Image:
                allow_stretch: True
                source: "Resistive_divider2.svg.png"
                pos_hint: {"x": .25, "y": .25}
                size_hint: .6, .6
            Label:
                id: lbl_Vin
                background_color: 1, 1, 1, .3
                text: ""
                base_font_size: '30sp'
                size_hint: .15, .1
                pos_hint: {"x": .28, "y": .76}
            Label:
                id: lbl_R1
                background_color: 1, 1, 1, .3
                text: ""
                base_font_size: '30sp'
                size_hint: .15, .1
                pos_hint: {"x": .35, "y": .64}
            Label:
                id: lbl_R2
                background_color: 1, 1, 1, .3
                text: ""
                base_font_size: '30sp'
                size_hint: .15, .1
                pos_hint: {"x": .35, "y": .4}
            Label:
                id: lbl_Vout
                background_color: 1, 1, 1, .3
                text: "voltage"+"V"
                base_font_size: '30sp'
                size_hint: .15, .1
                pos_hint: {"x": .68, "y": .51}

            Label:
                background_color: 1, 1, 1, .3
                text: "Voltage input (V):"
                base_font_size: '30sp'
                size_hint: .15, .1
                pos_hint: {"x": .02, "y": .75}
            Label:
                background_color: 1, 1, 1, .3
                text: " Resistance of R2 (Ω):"
                base_font_size: '30sp'
                size_hint: .15, .1
                pos_hint: {"x": .02, "y": .55}
            Label:
                background_color: 1, 1, 1, .3
                text: " Resistance of R1 (Ω):"
                base_font_size: '30sp'
                size_hint: .15, .1
                pos_hint: {"x": .02, "y": .35}
            TextInput:
                id: Vin_VD
                text: ''
                multiline: False
                size_hint: .15, .06
                pos_hint: {"x": .02, "y": .7}
            TextInput:
                id: R1_VD
                text: ''
                multiline: False
                size_hint: .15, .06
                pos_hint: {"x": .02, "y": .5}
            TextInput:
                id: R2_VD
                text: ''
                multiline: False
                size_hint: .15, .06
                pos_hint: {"x": .02, "y": .3}
            Button:
                id: btn_calc
                text: "Submit"
                font_size: '20sp'
                color: 1, 1, 1, .8
                size_hint: 0.2, 0.1
                pos_hint: {"x": .02, "y": .1}
                on_press: lbl_Vin.text = Vin_VD.text+"V"
                on_press: lbl_R1.text = R1_VD.text+"Ω"
                on_press: lbl_R2.text = R2_VD.text+"Ω"
                #on_press: root.textinis(Vin_VD.text, R1_VD.text, R2_VD.text)
                #on_press: root.textinis()
#---------------------------------------------ketvirtas tabas------------------------------------------
    TabbedPanelItem:
        text: 'LM 317'
        canvas:
            Color:
                rgba: 1, 1, 1, .6
            Rectangle:
                size: self.size
                pos: self.pos
        FloatLayout:
            Image:
                allow_stretch: True
                source: "LM317.png"
                pos_hint: {"x": .2, "y": .2}
                size_hint: .7, .7
            Label:
                id: r1
                background_color: 1, 1, 1, .3
                text: "R1"
                base_font_size: '30sp'
                size_hint: .15, .1
                pos_hint: {"x": .64, "y": .59}
            Label:
                id: r2
                background_color: 1, 1, 1, .3
                text: "R2"
                base_font_size: '30sp'
                size_hint: .15, .1
                pos_hint: {"x": .64, "y": .4}
            Label:
                id: volt
                background_color: 1, 1, 1, .3
                text: "Vout"
                base_font_size: '30sp'
                size_hint: .15, .1
                pos_hint: {"x": .73, "y": .7}
            TextInput:
                id: R1_LM
                text: ''
                multiline: False
                size_hint: .15, .06
                pos_hint: {"x": .02, "y": .5}
            TextInput:
                id: R2_LM
                text: ''
                multiline: False
                size_hint: .15, .06
                pos_hint: {"x": .02, "y": .3}
            Button:
                id: sub
                text: "Submit"
                font_size: '20sp'
                color: 1, 1, 1, .8
                size_hint: 0.2, 0.1
                pos_hint: {"x": .02, "y": .1}
                on_press: r1.text = R1_LM.text+"Ω"
                on_press: r2.text = R2_LM.text+"Ω"
                #on_press: root.textinis(Vin_VD.text, R1_VD.text, R2_VD.text)
                #on_press: root.textinis()