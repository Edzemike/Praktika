#: import FloatLayout kivy.uix.floatlayout.FloatLayout

<Mygtukas@Button>:
    font_size: '20sp'
    color: 1, 1, 1, .8
    size_hint: 0.2, 0.1

<MygtukasV@Button>:
    color: 0, 1, 0, 1
    size_hint: 0.04, .19
    background_color: 0, .7, 0, .3

<PopMyg@Button>:
    background_normal: ''

<TextInput>:
    multiline: False
    size_hint: 0.2, 0.06

<button_34@Button>:
    font_size: '20sp'
    color: 1, 1, 1, .8
    size_hint: .2, .08


<Tabbedpanel>:
    canvas:
        Color:
            rgba: 1, 1, 1, .7
        Rectangle:
            size: self.size
            pos: self.pos
    size_hint: 1, 1 #procentai: 1 - 100%, .5 - 50%
    pos_hint: {'center_x': .5, 'center_y': .5}
    do_default_tab: False
    #--------------------------------------------------pirmas tabas--------------------------------------------
    TabbedPanelItem:
        text: 'Value'
        on_press: tarpine.text = ''
        on_press: VarzMyg1.background_color = (0, .7, 0, .3)
        on_press: VarzMyg2.background_color = (0, .7, 0, .3)
        on_press: VarzMyg3.background_color = (0, .7, 0, .3)
        on_press: VarzMyg4.background_color = (0, .7, 0, .3)
        on_press: VarzMyg5.background_color = (0, .7, 0, .3)
        on_press: VarzMyg6.background_color = (0, .7, 0, .3)
        on_press: VarzMyg6.disabled = False
        on_press: VarzMyg3.disabled = False
        on_press: VarzMyg4.pos_hint = {"x": 0.56, 'y': .455}
        on_press: VarzMyg5.pos_hint = {"x": 0.63, 'y': .455}
        on_press: temperature.text = ''
        on_press: tolerance.text = ''
        on_press: resistance.text = ''

        canvas:
            Color:
                rgba: 1, 1, 1, .6
            Rectangle:
                size: self.size
                pos: self.pos
        FloatLayout:
            RstDocument:
                text: "Calculates the resistance value according to color-code."
                size_hint: 1, .1

            Mygtukas:
                id: keturiosj
                text: "4-band-code"
                pos_hint: {"x": 0, 'y': .9}
                on_press: VarzMyg3.disabled=True
                on_press: VarzMyg6.disabled=True
                on_press: VarzMyg3.background_color=(0, .7, 0, 0)
                on_press: VarzMyg6.background_color=(0, .7, 0, 0)
                on_press: VarzMyg4.pos_hint={"x": 0.49, 'y': .455}
                on_press: VarzMyg5.pos_hint={"x": 0.67, 'y': .455}
                on_press: submit_4band_code.text = "Submit"
                on_press: submit_4band_code.background_color = (1, 1, 1, .5)
                on_press: submit_56band_code.text = ''
                on_press: submit_56band_code.background_color = (1, 1, 1, 0)

            Mygtukas:
                id: penkiosj
                text: "5-band-code"
                pos_hint: {"x": .2, 'y': .9}
                on_press: VarzMyg3.disabled=False
                on_press: VarzMyg6.disabled=True
                on_press: VarzMyg3.background_color=(0, .7, 0, .3)
                on_press: VarzMyg6.background_color=(0, .7, 0, 0)
                on_press: VarzMyg4.pos_hint={"x": 0.56, 'y': .455}
                on_press: VarzMyg5.pos_hint={"x": .67, 'y': .455}

            Mygtukas:
                id: sesiosj
                text: "6-band-code"
                pos_hint: {"x": .4, 'y': .9}
                on_press: VarzMyg3.disabled=False
                on_press: VarzMyg6.disabled=False
                on_press: VarzMyg3.background_color=(0, .7, 0, .3)
                on_press: VarzMyg6.background_color=(0, .7, 0, .3)
                on_press: VarzMyg4.pos_hint={"x": 0.56, 'y': .455}
                on_press: VarzMyg5.pos_hint={"x": 0.63, 'y': .455}

            Image:
                allow_stretch: True
                source: "Resistor_symbol.png"
                pos_hint: {"x": 0.25, 'y': .25}
                size_hint: .6, .6

            Label:
                id: resistance
                background_color: 1, 1, 1, .3
                text: ''
                base_font_size: '30sp'
                size_hint: .15, .1
                pos_hint: {"x": .47, "y": .31}
            Label:
                id: tolerance
                background_color: 1, 1, 1, .3
                text: ''
                base_font_size: '30sp'
                size_hint: .15, .1
                pos_hint: {"x": .47, "y": .21}
            Label:
                id: temperature
                background_color: 1, 1, 1, .3
                text: ''
                base_font_size: '30sp'
                size_hint: .15, .1
                pos_hint: {"x": .47, "y": .11}
